{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sync:backend": "tsx --no-warnings ./ops/syncWithBackend.js",
    "env:prod": "npx cross-env NODE_ENV=production",
    "env:dev": "npx cross-env NODE_ENV=development",
    "env:test": "npx cross-env NODE_ENV=test",
    "env:create:samples": "tsx --no-warnings ./ops/createEnvSamples.js",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check:format": "npx prettier --check \"./src/**/*.{ts,tsx}\"",
    "check:eslint": "eslint",
    "check:types": "npx tsc --noEmit",
    "check": "npm run check:format && npm run check:eslint && npm run check:types",
    "fix:format": "npx prettier --write \"./src/**/*.{ts,tsx}\"",
    "test": "npm run test:build && npm run start",
    "test:build": "npm run env:test -- next build",
    "test:start": "npm run env:test -- next start",
    "test:jest": "npm run env:test -- jest",
    "test:cypress:open": "npm run env:test -- cypress open",
    "test:cypress:run": "npm run env:test -- cypress run"
  },
  "dependencies": {
    "@better-fetch/fetch": "^1.1.21",
    "@mantine/charts": "^8.3.14",
    "@mantine/core": "^8.3.14",
    "@mantine/dates": "^8.3.14",
    "@mantine/hooks": "^8.3.14",
    "@mantine/notifications": "^8.3.14",
    "@mantine/tiptap": "^8.3.14",
    "@tiptap/extension-highlight": "^3.19.0",
    "@tiptap/extension-link": "^3.19.0",
    "@tiptap/extension-subscript": "^3.19.0",
    "@tiptap/extension-superscript": "^3.19.0",
    "@tiptap/extension-task-item": "^3.19.0",
    "@tiptap/extension-task-list": "^3.19.0",
    "@tiptap/extension-text-align": "^3.19.0",
    "@tiptap/pm": "^3.19.0",
    "@tiptap/react": "^3.19.0",
    "@tiptap/starter-kit": "^3.19.0",
    "@vercel/blob": "^2.2.0",
    "better-auth": "^1.4.18",
    "clsx": "^2.1.1",
    "cross-env": "^10.1.0",
    "dayjs": "^1.11.19",
    "decode-formdata": "^0.9.0",
    "immer": "^11.1.3",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "recharts": "^3.7.0",
    "valibot": "^1.2.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "cypress": "^15.10.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "10.1.8",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.8.1",
    "sass": "^1.97.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
